<?xml version="1.0" encoding="utf-8"?>


    <androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <View
            android:id="@+id/rectangle_2"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:background="@drawable/rectangle_2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.558"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/lg" />

    <TextView
        android:id="@+id/Resgiter"
        android:layout_width="120dp"
        android:layout_height="29dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="10dp"
        android:gravity="top"
        android:lineSpacingExtra="9sp"
        android:text="REGISTER"
        android:textAppearance="@style/login"
        android:translationY="-4.35sp"
        app:layout_constraintBottom_toTopOf="@+id/group_24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.562"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.362" />

    <View
            android:id="@+id/group_24"
            android:layout_width="321dp"
            android:layout_height="270dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginBottom="16dp"
            android:background="#FFFFFFFF"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.633"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/form_passwo"
            android:layout_width="281dp"
            android:layout_height="45dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="28dp"
            android:background="#71ADDD"
            android:gravity="center"
            android:hint="PASSWORD"
            app:layout_constraintEnd_toEndOf="@+id/group_24"
            app:layout_constraintStart_toStartOf="@+id/group_24"
            app:layout_constraintTop_toBottomOf="@+id/form_email" />

        <EditText
            android:id="@+id/form_email"
            android:layout_width="281dp"
            android:layout_height="45dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="10dp"
            android:background="#71ADDD"
            android:gravity="center"
            android:hint="EMAIL"
            app:layout_constraintEnd_toEndOf="@+id/group_24"
            app:layout_constraintStart_toStartOf="@+id/group_24"
            app:layout_constraintTop_toTopOf="@+id/group_24" />

    <EditText
        android:id="@+id/form_no_hp"
        android:layout_width="281dp"
        android:layout_height="45dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="144dp"
        android:background="#71ADDD"
        android:gravity="center"
        android:hint="Ketik Ulang Password"
        app:layout_constraintEnd_toEndOf="@+id/group_24"
        app:layout_constraintStart_toStartOf="@+id/group_24"
        app:layout_constraintTop_toTopOf="@+id/group_24" />

    <Button
            android:id="@+id/tombol_register"
            android:layout_width="281dp"
            android:layout_height="45dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginBottom="8dp"
            android:background="#71ADDD"
            android:gravity="center"
            android:text="@string/register"
            android:textColor="#FEFCFF"
            app:layout_constraintBottom_toBottomOf="@+id/group_24"
            app:layout_constraintEnd_toEndOf="@+id/group_24"
            app:layout_constraintStart_toStartOf="@+id/group_24"
            app:layout_constraintTop_toBottomOf="@+id/tombol_logi" />

    </androidx.constraintlayout.widget.ConstraintLayout>

